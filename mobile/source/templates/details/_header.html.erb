<header class="bar bar-nav">
  <a class="react action pull-left" ng-click="back()">
    <span class="ionicon ion-chevron-left">
  </a>
  <h1 class="title">{{data.name || "详细信息"}} </h1>
  <a class="react action pull-right" ng-click="favorite(type, id)">
    <span class="ionicon ion-star">
  </a>
  <a class="react action pull-right">
    <span class="ionicon ion-share" ng-click="share(data)">
  </a>
</header>